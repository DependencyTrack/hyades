{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "/src/main/resources/application\\.properties$"
      ],
      "matchStrings": [
        "^quarkus\\.kafka\\.devservices\\.image-name=(?<depName>[^:]+):(?<currentValue>[^\"'\\s@]+)@?(?<currentDigest>sha256:[a-f0-9]+)?(\\s|$)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
